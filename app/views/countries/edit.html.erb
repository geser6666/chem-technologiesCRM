<% provide(:title, 'Редактирвоание информации о стране') %>
<h1>Редактирвоание информации о стране</h1>
<div class="row">
        <div class="span6 offset3">
                <%= form_for(@country) do |f| %>
                        <%= render 'fields', f:f %>
                        <%= f.submit "Сохранить", class: "btn btn-large btn-primary" %>
                <% end %>
        </div>
</div>